<!DOCTYPE html>
<html  ng-app="yoB">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@string.Format(System.Configuration.ConfigurationManager.AppSettings["LayoutTitle"] ?? "{0}", @ViewBag.Title ?? string.Empty)</title>
    <!-- 新 Bootstrap 核心 CSS 文件 -->
    <link href="@System.Configuration.ConfigurationManager.AppSettings["ServerReferenceUserCenter"]/Content/bootstrap.min.css" type="text/css" rel="stylesheet" >
    <link href="@System.Configuration.ConfigurationManager.AppSettings["ServerReferenceUserCenter"]/Content/global.css" type="text/css" rel="stylesheet">
    <!-- 可选的Bootstrap主题文件（一般不用引入） -->
    @*<link rel="stylesheet" href="//cdn.bootcss.com/bootstrap/3.0.3/css/bootstrap-theme.min.css">*@
    @RenderSection("csslink", required: false)
    <link rel="stylesheet" type="text/css" href="~/Content/css/common.css"  />
    @*<link rel="stylesheet" type="text/css" href="~/Content/css/more.css"  />*@
    <link href="~/Content/global.css" type="text/css" rel="stylesheet">
    <script src="~/Configuration/Javascript"></script>
    <script src="@System.Configuration.ConfigurationManager.AppSettings["ServerReferenceUserCenter"]/Scripts/jquery.min.js"></script>
    <script src="@System.Configuration.ConfigurationManager.AppSettings["ServerReferenceUserCenter"]/Scripts/bootstrap.min.js"></script>

    <script src="~/Scripts/js/lib/jquery-migrate-1.3.0.js"></script>
    <script type="text/javascript">
        var UT ={}, UI={}, imgUrl = ucConfig.ServerReferenceFileCenterAPI + '/fileupload/ImgDownload/', ImgSingleUploadUrl = ucConfig.ServerReferenceFileCenterAPI + '/fileupload/ImgSingleUpload/CrowdSourcing';
    </script>
    <!--[if lt IE 9]>
        <script src="//cdn.bootcss.com/es5-shim/4.0.3/es5-shim.min.js"></script>
        <script src="//cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/lodash.js"></script>
    <script src="~/Scripts/js/uc.ut.js" type="text/javascript"></script>
    <script src="~/Scripts/js/uc.ui.js" type="text/javascript"></script>
    @RenderSection("scriptlink", required: false)
</head>
<body style="background-color:rgb(245, 245, 245);">
    @RenderSection("FrameHeader", required: false)
    @RenderSection("FrameNav", required: false)
    @RenderSection("FrameLeftAside", required: false)
    <div class="container framesection">
        @RenderBody()
    </div>
    @RenderSection("FrameRightAside", required: false)
    @RenderSection("FrameFooter", required: false)
    @RenderSection("scripts", required: false)
</body>
</html>
