        <div class="form-group">
            <label for="UserName" class="col-sm-2 control-label">手机：</label>                
            <div class="col-sm-3">                                        
                @Html.TextBoxFor(model => model.UserName, new { @class = "form-control", placeholder="请输入手机号" , ng_model="geRen.UserName", email_phone="", unique_username="", required="", form="reg1Form" })
            </div>
            <div class="col-sm-7">                                        
                <span class="merror" ng-show="reg1Form.UserName.$dirty && reg1Form.UserName.$invalid">
                    <span ng-show="reg1Form.UserName.$error.required">必须填写</span>
                    <span ng-show="reg1Form.UserName.$error.emailPhone">请填写正确的手机号</span>                    
                    <span ng-show="reg1Form.UserName.$error.uniqueUsername">已经被注册，可直接登录</span>                    
                </span>
            </div>
        </div>
        <div class="form-group">
            <label for="Password" class="col-sm-2 control-label">@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:Password")</label>
