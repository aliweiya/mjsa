                @Html.TextBoxFor(model => model.Password, new { @class = "form-control", placeholder= Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:EnterPassword"), Type = "Password", ng_model="geRen.Password", ng_minlength="6", ng_maxlength="14", required="", form="reg1Form"})
            </div>
            <div class="col-sm-7">
                <span class="merror" ng-show="reg1Form.Password.$dirty && reg1Form.Password.$invalid">
                    <span ng-show="reg1Form.Password.$error.required">必须填写</span>                   
                    <span ng-show="reg1Form.Password.$error.minlength">最少6个字符</span>                   
                    <span ng-show="reg1Form.Password.$error.maxlength">最多14个字符</span>  
                    <span ng-show="reg1Form.Password.$error.equalPassword">两次输入的密码不同</span>                  
                </span>
            </div>
        </div>
        <div class="form-group">
            <label for="RePassword" class="col-sm-2 control-label">@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:RePassword")</label>
