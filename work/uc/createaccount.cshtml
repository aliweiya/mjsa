            @Html.TextBoxFor(model => model.RePassword, new { @class = "form-control", placeholder= Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:ReEnterPassword"), Type = "Password", ng_model="geRen.RePassword", ng_minlength="6", ng_maxlength="14", required="", equal_password="Password", form="reg1Form"})
            </div>
            <div class="col-sm-7">
                <span class="merror" ng-show="reg1Form.RePassword.$dirty && reg1Form.RePassword.$invalid">
                    <span ng-show="reg1Form.RePassword.$error.required">必须填写</span>                   
                    <span ng-show="reg1Form.RePassword.$error.minlength">最少6个字符</span>                   
                    <span ng-show="reg1Form.RePassword.$error.maxlength">最多14个字符</span>                   
                    <span ng-show="reg1Form.RePassword.$error.equalPassword">两次输入的密码不同</span>                   
                </span>
            </div>
        </div>
        <div class="form-group">
            <label for="Vcode" class="col-sm-2 control-label">@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:ValidateCode")</label>
            <div class="col-sm-3">
                @Html.TextBoxFor(model => model.Vcode, new { @class = "form-control", placeholder= Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:EnterValidateCode"), ng_model="geRen.Vcode", v_vcode="", required="" }) 

            </div>
            <div class="col-sm-2">
                <div class="ivcode">                
                    <img class="btn btn-info vvcode" ng-click="getVcode($event)" src="../Account/GetSafeValidateCode"/>
                </div>                                        
            </div>
            <div class="col-sm-5">
                <span class="merror" ng-show="reg1Form.Vcode.$dirty && reg1Form.Vcode.$invalid">
                    <span ng-show="reg1Form.Vcode.$error.vVcode">验证码不正确</span>                 
                </span>
            </div>
        </div> 

        <div class="SMSdlDcode form-group" ng-if="showSms()">
            <label for="Dcode" class="col-sm-2 control-label">@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:DynamicCode")</label>
            <div class="col-sm-3">
                @Html.TextBoxFor(model => model.Dcode, new { @class = "form-control", placeholder= Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:EnterDynamicCode"), ng_model="geRen.Dcode", v_dcode="" , required=""})
            </div>
            <div class="col-sm-1">
                <div class="SMSdl" ng-show="showSms()">
                    <input class="btn btn-info" type="button" ng-click="getDcode($event, geRen.UserName)" value='@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:GetValidateCode")' />
                </div>
            </div>
            <div class="col-sm-6">
                <span class="merror" ng-show="reg1Form.Dcode.$dirty && reg1Form.Dcode.$invalid">
                    <span ng-show="reg1Form.Dcode.$error.vDcode">验证码不正确</span>                 
                </span>
            </div>
        </div>        
       
        <div class="form-group">
            <div class="col-sm-5 col-sm-offset-2">
                <div class="checkbox">
                    <label>                                            
                        <input  class="ckbprotocal" name="ckbprotocal" ng-model="geRen.ckbprotocal" type="checkbox" type="checkbox"> @Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:AgreeAndAccept") <a class="ckb" target="_blank" href='@string.Format("{0}{1}", System.Configuration.ConfigurationManager.AppSettings["ServerReferenceUserCenter"], "/Account/Protocal")'>@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:UserProtocal")</a>
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-10 col-sm-offset-2">
                <input type="submit" class="btnLogin btn btn-primary" ng-disabled="reg1Disable()" value='@Html.I18NTranslate("ZCPT.PublicPortal.Web.Account.CreateAccount:RegisterNow")'/>
            </div>
        </div>
    </form>
</script>
