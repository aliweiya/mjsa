{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "destructuring": true,
    "defaultParams": true,
    "objectLiteralShorthandProperties": true,
    "objectLiteralShorthandMethods": true,
    "restParams": true,
    "spread": true
  },
  "globals": {
    "Bacon": true,
    "Observable": true,
    "Exception": true,
    "nextEvent": true,
    "endEvent": true,
    "withDescription": true,
    "withDesc": true
  },
  "rules": {
    "strict": 0
  }
}
