{
    "browser": true,
    "esnext": true,
    "bitwise": false,
    "curly": false,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "latedef": false,
    "laxcomma": true,
    "newcap": true,
    "noarg": true,
    "undef": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "white": true,
    "indent": 4,
    "predef": [
        "describe",
        "it",
        "expect",
        "runs",
        "waitsFor",
        "beforeEach",
        "afterEach",
        "module",
        "provide",
        "define"
    ]
}
